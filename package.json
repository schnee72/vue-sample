{
  "name": "vue-sample",
  "version": "1.0.0",
  "description": "Vue JS sample project",
  "repository": "https://github.com/schnee72/vue-sample.git",
  "author": "Dustin Schnee <schnee72@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "npm-run-all --parallel open:dev lint:watch",
    "open:dev": "babel-node tools/server.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^4.2.2",
    "eslint": "^5.2.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "mini-css-extract-plugin": "^0.10.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "open": "^6.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^10.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
